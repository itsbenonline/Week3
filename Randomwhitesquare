
 int hori = 20;
 int verti = 10;

 void setup() {
  size (400, 400);
  background (255);
  stroke (0);
  //rectMode(CENTER);   
  for (int i=0; i<25; i++) { //verticle. 
    for (int j=0; j<25; j++) {//horizontal... need to add randomness. 
      if (random(7) > 3) {
        rect(hori+(i*20), hori+(j*20), verti, verti);
      }
    }
  }
 }
